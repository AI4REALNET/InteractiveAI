{
  "name": "cab-front",
  "version": "4.0.0-alpha6",
  "private": true,
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:": "vite --mode",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:": "vite build --mode",
    "preview": "vite preview",
    "test": "run-p 'test:unit -- run' test:e2e",
    "test:coverage": "run-p test:unit:coverage test:e2e:coverage",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:e2e:coverage": "npm run test:e2e && nyc report --reporter=text-summary",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:svg": "svgo -rf src public/img",
    "format": "prettier --write src/",
    "commit": "commit",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "d3": "^7.8.5",
    "date-fns": "^3.3.1",
    "leaflet": "^1.9.4",
    "lucide-vue-next": "0.x",
    "mitt": "^3.0.1",
    "modern-normalize": "^2.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "three": "^0.161.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/prompt-cli": "^18.4.3",
    "@cypress/code-coverage": "^3.12.19",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^21.1.3",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.10.0",
    "@types/three": "^0.161.2",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.3.1",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-simple-import-sort": "^11.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.2",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.1",
    "svgo": "^3.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.19"
  }
}
